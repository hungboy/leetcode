{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "TS-node launch current file",
      "protocol": "inspector",
      "internalConsoleOptions": "openOnSessionStart",
      "args": ["${file}"],
      "runtimeArgs": ["-r", "ts-node/register"]
    },
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "TS-Node",
      "args": ["src/index.ts"],
      "runtimeArgs": ["-r", "ts-node/register"],
      "cwd": "${workspaceFolder}",
      "protocol": "inspector",
      "internalConsoleOptions": "openOnSessionStart"
    }
  ],
  "compounds": []
}
